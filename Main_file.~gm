
variable x(i),d(i);
parameter a(i);
a(i) = normal(1000,100);

parameter y(i);
y(i) = normal(1500,200);

P=8000;
x.lo(i)=0;



equations Objective,powbal,con1(i),con4(i),con5,con6,con2(i);


Objective..  O=e=t;
powbal..  sum(i,x(i))=e=P;
con1(i)..  d(i)=e=a(i)-x(i);
con4(i)..  x(i)=l=y(i);
con5..  t+dummy=c=sum(i,d(i));
con6..  dummy=e=0.5

model unitcommitment /all/;
option LP=MOSEK;
solve unitcommitment minimizing O using LP;
unitcommitment.optfile=1;
display x.l,O.l;


p=500;
option LP=MOSEK;
solve unitcommitment minimizing O using LP;
unitcommitment.optfile=1;
display x.l,O.l;

p=540;
option LP=MOSEK;
solve unitcommitment minimizing O using LP;
unitcommitment.optfile=1;
display x.l,O.l;
